%@article{Garcia:2008OSA, 
%author = {Javier Garc\'{i}a and Zeev Zalevsky and Pascuala Garc\'{i}a-Mart\'{i}nez and Carlos Ferreira and Mina Teicher and Yevgeny Beiderman}, 
%journal = {Appl. Opt.}, 
%keywords = {Speckle; Three-dimensional image processing; Speckle imaging},
%number = {16}, 
%pages = {3032--3040}, 
