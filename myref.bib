%@article{Grosse:2010Macro, 
%author = {Marcus Grosse and Johannes Buehl and Holger Babovsky and Armin Kiessling and Richard Kowarschik}, 
%journal = {Opt. Lett.}, 
%keywords = {Three-dimensional sensing; Digital holography ; Optical sensing and sensors},
